{
  "name": "WeFlow",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "build:all": "electron-packager . --all --overwrite --out ./dist",
    "build:mac": "electron-packager ./ WeFlow Mac --platform=darwin --arch=x64 --icon=./assets/img/WeFlow.icns --overwrite --out ./dist --version=0.37.8",
    "build:win64": "electron-packager ./ WeFlow --platform=win32 --arch=x64 --icon=./assets/img/WeFlow.png --overwrite --out ./dist --version=0.37.8",
    "build:win32": "electron-packager ./ WeFlow --platform=win32 --arch=ia32 --icon=./assets/img/WeFlow.png --overwrite --out ./dist --version=0.37.8 --app-version=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weixin/WeFlow"
  },
  "keywords": [
    "Electron",
    "workflow",
    "F2E",
    "GUI"
  ],
  "author": "GitHub",
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/weixin/WeFlow/issues"
  },
  "homepage": "https://github.com/weixin/WeFlow#readme",
  "devDependencies": {
    "electron-packager": "^7.0.2",
    "electron-prebuilt": "^0.37.0"
  },
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "autoprefixer": "^6.3.3",
    "browser-sync": "^2.11.1",
    "crypto-md5": "^1.0.0",
    "del": "^2.2.0",
    "electron-prebuilt": "^0.37.8",
    "extract-zip": "^1.5.0",
    "gulp": "^3.9.1",
    "gulp-cssnano": "^2.1.1",
    "gulp-ejs": "^2.1.1",
    "gulp-ftp": "^1.1.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-lazyimagecss": "^2.0.0",
    "gulp-less": "^3.0.5",
    "gulp-postcss": "^6.0.1",
    "gulp-posthtml": "^1.5.2",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rev-all": "^0.8.22",
    "gulp-rev-delete-original": "^0.1.0",
    "gulp-tmtsprite": "^0.0.20",
    "gulp-uglify": "^1.5.3",
    "gulp-usemin2": "^0.2.4",
    "gulp-util": "^3.0.7",
    "gulp-webp": "^2.3.0",
    "gulp-zip": "^3.2.0",
    "imagemin-pngquant": "^4.2.2",
    "lodash": "^4.5.1",
    "postcss-pxtorem": "^3.3.1",
    "posthtml-px2rem": "^0.0.3",
    "rc": "^1.1.6",
    "rd": "^0.0.2",
    "tmt-ejs-helper": "^0.0.1",
    "vinyl-fs": "^2.4.3"
  }
}
